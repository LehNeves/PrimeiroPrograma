<Window x:Class="Oficial.views.AdicionarCarro"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Oficial.views"
        mc:Ignorable="d"
        Title="AdicionarCarro" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="modeloTextBox" HorizontalAlignment="Left" Height="23" Margin="229,156,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="anoTextBox" HorizontalAlignment="Left" Height="23" Margin="491,156,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.517,0.505" MaxLength="4"/>
        <Button x:Name="salvar" Content="Salvar" HorizontalAlignment="Left" Margin="656,340,0,0" VerticalAlignment="Top" Width="75" Click="SalvarCarro"/>
        <Label Content="Modelo" HorizontalAlignment="Left" Margin="138,156,0,0" VerticalAlignment="Top"/>
        <Label Content="Ano" HorizontalAlignment="Left" Margin="415,156,0,0" VerticalAlignment="Top"/>
        <Button Content="Cancelar" Click="Fechar" HorizontalAlignment="Left" Margin="516,340,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</Window>
